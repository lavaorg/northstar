{
  "container": {
    "docker": {
      "portMappings": [{
        "containerPort": 8080,
        "hostPort": 0,
        "servicePort": 0,
        "protocol": "tcp"
      }]
    }
  },
  "healthChecks": [{
    "protocol": "HTTP",
    "path": "/management/health",
    "gracePeriodSeconds": 3,
    "intervalSeconds": 10,
    "portIndex": 0,
    "timeoutSeconds": 10,
    "maxConsecutiveFailures": 3
  }],
  "env": {
    "KAFKA_BROKERS_HOST_PORT": "@KAFKA_BROKERS_HOST_PORT@",
    "KAFKA_ZK_URL": "@KAFKA_ZK_URL@",
    "LOGGER_KAFKA_BROKERS_HOST_PORT": "@LOGGER_KAFKA_BROKERS_HOST_PORT@",
    "LOGGER_ZOOKEEPER_HOST_PORT": "@LOGGER_ZOOKEEPER_HOST_PORT@",
    "ZOOKEEPER_HOST_PORT": "@ZOOKEEPER_HOST_PORT@",
    "DATA_HOST_PORT": "@DATA_HOST_PORT@",
    "CRON_HOST_PORT": "@CRON_HOST_PORT@",
    "PROCESSING_HOST_PORT": "@PROCESSING_HOST_PORT@",
    "KAFKA_MNGR_HOST_PORT": "@KAFKA_MNGR_HOST_PORT@",
    "ENABLE_DEBUG": "@ENABLE_DEBUG@",
    "THINGSPACE_PROTOCOL": "http",
    "THINGSPACE_SOUTH_HOST_PORT": "@THINGSPACE_SOUTH_HOST_PORT@",
    "THINGSPACE_AUTH_HOST_PORT": "@THINGSPACE_AUTH_HOST_PORT@",
    "THINGSPACE_DATA_HOST_PORT": "@THINGSPACE_DATA_HOST_PORT@",
    "THINGSPACE_CLIENT_ID": "@CLIENT_ID@",
    "THINGSPACE_SECRET": "@CLIENT_SECRET@",
    "SCOPES_FILENAME": "/northstarapi/scopes.json",
    "OBJECT_HOST_PORT": "@OBJECT_HOST_PORT@",
    "NORTHSTARAPI_MAX_ARG_COUNT": "@NORTHSTARAPI_MAX_ARG_COUNT@",
    "NORTHSTARAPI_MAX_TIMEOUT": "@NORTHSTARAPI_MAX_TIMEOUT@",
    "NORTHSTARAPI_MAX_CODE_SIZE": "@NORTHSTARAPI_MAX_CODE_SIZE@",
    "NORTHSTARAPI_ENFORCE_CHECKSUM":"@NORTHSTARAPI_ENFORCE_CHECKSUM@",
    "EXECUTION_MEMORY_MAX":"@NORTHSTARAPI_EXECUTION_MEMORY_MAX@",
    "EXECUTION_MEMORY_DEFAULT":"@NORTHSTARAPI_EXECUTION_MEMORY_DEFAULT@",
    "DPE_STREAM_HOST_PORT":"@DPE_STREAM_HOST_PORT@"

  }
}
