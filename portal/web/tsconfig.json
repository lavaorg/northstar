{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": ["es5", "dom"],
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "allowUnusedLabels": false,
        "removeComments": false,
        "suppressImplicitAnyIndexErrors": true,
        "allowJs": true,
        "skipLibCheck": true
    }
}